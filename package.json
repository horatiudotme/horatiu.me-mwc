{
  "name": "horatiu.me-mwc",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "scripts": {
    "start": "npm-run-all --parallel dev:*",
    "dev:eleventy": "cross-env ELEVENTY_ENV=development eleventy --serve",
    "dev:parcel": "parcel watch src/_assets/images/* src/_assets/css/main.scss src/_assets/js/main.js --out-dir _site_dev/assets",
    "build": "run-s prod:*",
    "prod:eleventy": "cross-env ELEVENTY_ENV=production eleventy --output=./_site_prod",
    "prod:parcel": "parcel build src/_assets/images/* src/_assets/css/main.scss src/_assets/js/main.js --out-dir _site_prod/assets",
    "clean": "run-s -c clean:*",
    "clean:dev": "rm -r _site_dev",
    "clean:prod": "rm -r _site_prod",
    "clean:cache": "rm -r .parcel-cache"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "@11ty/eleventy": "^0.11.0",
    "cross-env": "^7.0.2",
    "npm-run-all": "^4.1.5",
    "parcel": "^1.12.4",
    "sass": "^1.27.0"
  },
  "dependencies": {
    "@material/mwc-button": "^0.19.1",
    "@material/mwc-icon-button": "^0.19.1",
    "@material/mwc-list": "^0.19.1",
    "@material/mwc-tab": "^0.19.1",
    "@material/mwc-tab-bar": "^0.19.1",
    "@material/mwc-top-app-bar-fixed": "^0.19.1",
    "@material/typography": "^7.0.0",
    "js-yaml": "^3.14.0"
  }
}
